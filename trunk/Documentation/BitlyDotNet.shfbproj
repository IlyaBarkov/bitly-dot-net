      <DocumentationSource sourceFile="..\BitlyDotNET\bin\Debug\BitlyDotNET.dll" />
      <DocumentationSource sourceFile="..\BitlyDotNET\bin\Debug\BitlyDotNET.xml" />
    </DocumentationSources>
