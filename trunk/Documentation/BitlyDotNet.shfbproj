      <NamespaceSummaryItem name="BitlyDotNET.Interfaces" isDocumented="True">Defines an easy way to communicate with the bit.ly API in .NET</NamespaceSummaryItem>
    </NamespaceSummaries>
    <RootNamespaceContainer>True</RootNamespaceContainer>
